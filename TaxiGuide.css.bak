}

 .grid-container blockquote { 
  display: flex;
    flex-direction: column-reverse;
    width: 121%;
    height: 76%;
    margin-left: 30px;
    padding: 30px;
    text-align: center
}
blockquote p {
    border-radius: 5px;
    text-align: center;
    font-size: 17px;
    line-height: 1.4
}

blockquote p::before {
  content: '\201C';
  color:#e68a00;
  size: 10px;
}

blockquote p::after {
  content: '\201D';
  color:#e68a00;
  size: 10px;
}

