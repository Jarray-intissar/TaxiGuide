@-webkit-keyframes glow {
  from {
    text-shadow: 0 0 10px #fff, 0 0 20px #fff, 0 0 30px #e68a00, 0 0 40px orange, 0 0 50px orange, 0 0 60px orange, 0 0 70px orange;
  }
  
  to {
    text-shadow: 0 0 20px #fff, 0 0 30px rgba(255, 145, 0, 0.74), 0 0 40px rgba(255, 145, 0, 0.74), 0 0 50px rgba(255, 145, 0, 0.74), 0 0 60px rgba(255, 145, 0, 0.74), 0 0 70px rgba(255, 145, 0, 0.74), 0 0 80px rgba(255, 145, 0, 0.74);
  }
}
